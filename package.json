{
  "name": "node-es6",
  "version": "1.0.0",
  "description": "starter template for ES6 syntaxes",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/server.js",
    "build": "babel src -d dist",
    "start": "npm run build && nodemon dist/server.js",
    "serve": "npm run build && node dist/server.js",
    "serve:pm2": "npm run build && pm2 start dist/server.js",
    "test": "npm run build && mocha --require @babel/register",
    "clear:linux": "rm -rf dist",
    "clear:win": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmansimli/node-es6.git"
  },
  "keywords": [],
  "author": "Farid Mansimli <farid.mansimli@faridmansim.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fmansimli/node-es6/issues"
  },
  "homepage": "https://github.com/fmansimli/node-es6#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.15.3",
    "@babel/runtime": "^7.14.8",
    "babel-preset-minify": "^0.5.1",
    "mocha": "^9.1.0",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2"
  }
}
